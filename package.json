    "anixartjs": "^0.0.7",
    "electron-serve": "^1.3.0",
    "electron-squirrel-startup": "^1.0.1",
    "lottie-react": "^2.4.0",
    "openurl": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}